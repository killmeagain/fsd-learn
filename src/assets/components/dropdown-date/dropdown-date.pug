mixin dropdown-date(props = {})
    -const {titleStart, titleFinish} = props
    .dropdown-date
        .dropdown-date__input-start
            include ../formInput/formInput
            +formInput({
                title: titleStart,
                dropdown: true,
                value: "ДД.ММ.ГГГГ",
                placeholder: "ДД.ММ.ГГГГ"
            })
        .dropdown-date__input-finish
            include ../formInput/formInput
            +formInput({
                title: titleFinish,
                dropdown: true,
                value: "19.08.2019",
                placeholder: "ДД.ММ.ГГГГ"
            })